<Defs>

	<ThingDef ParentName="BuildingBase" Name="VFEI2_ThumperBase" Abstract="True">
		<thingClass>Building</thingClass>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
			<shadowData>
				<volume>(1.29,1,1.69)</volume>
			</shadowData>
		</graphicData>
		<drawerType>MapMeshAndRealTime</drawerType>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.3</fillPercent>
		<pathCost>50</pathCost>
		<rotatable>false</rotatable>
		<blockWind>true</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<statBases>
			<MaxHitPoints>80</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<MarketValue>250</MarketValue>
			<Flammability>0.5</Flammability>
			<Mass>25</Mass>
		</statBases>
		<tickerType>Normal</tickerType>
		<canOverlapZones>false</canOverlapZones>
		<size>(2,2)</size>
		<building>
			<destroySound>BuildingDestroyed_Metal_Medium</destroySound>
		</building>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<constructEffect>ConstructMetal</constructEffect>
		<designationCategory>VFEI2_Insectoids</designationCategory>
		<killedLeavings>
			<ChunkSlagSteel>3</ChunkSlagSteel>
		</killedLeavings>
		<comps>
			<li Class="CompProperties_Usable">
				<useJob>TriggerObject</useJob>
				<useLabel>Activate thumper</useLabel>
				<useDuration>60</useDuration>
				<floatMenuFactionIcon>Insect</floatMenuFactionIcon>
			</li>
			<li Class="CompProperties_Explosive">
				<wickTicks>160~600</wickTicks>
				<explosiveRadius>5.9</explosiveRadius>
				<explosiveDamageType>Bomb</explosiveDamageType>
			</li>
		</comps>
	</ThingDef>

	<ThingDef ParentName="VFEI2_ThumperBase">
		<defName>VFEI2_Thumper_Empress</defName>
		<label>empress thumper</label>
		<description>A vibration-producing thumper device. When activated, it starts emitting powerful thump pulses, which will attract an insectoid empress to attack with its escorts. If you can defeat her, you can collect a special pherocore from her corpse. The thumps destabilise the thumper, so this building can only be used once.</description>
		<graphicData>
			<texPath>Things/Building/Thumpers/EmpressThumper_Base</texPath>
		</graphicData>
		<uiIconPath>Things/Building/Thumpers/EmpressThumper_MenuIcon</uiIconPath>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<researchPrerequisites>
			<li>VFEI2_StandardHivetech</li>
			<li>Electricity</li>
		</researchPrerequisites>
		<comps>
			<li Class="VFEInsectoids.CompProperties_Thumper">
				<topGraphic>
					<texPath>Things/Building/Thumpers/EmpressThumper_Top</texPath>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(2,2)</drawSize>
				</topGraphic>
				<wave>VFEI2_Thumper_Empress</wave>
			</li>
		</comps>
	</ThingDef>

